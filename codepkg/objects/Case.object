    <fields>
        <fullName>CTP_Late_Claim_Flag__c</fullName>
        <externalId>false</externalId>
        <formula>If(OR(AND(NOT(ISBLANK(CTP_LodgementDate__c)),CTP_LodgementDate__c &gt; DATE(YEAR(CTP_Date_of_accident__c)+ FLOOR((MONTH(CTP_Date_of_accident__c)+3-1)/12),MOD(MONTH(CTP_Date_of_accident__c)+3-1,12)+1,DAY(CTP_Date_of_accident__c))-1),
AND(NOT(ISBLANK(CTP_Date_of_Common_Law_Notification__c)),
CTP_Date_of_Common_Law_Notification__c&gt;IF( 
AND( 
MONTH(CTP_Date_of_accident__c) = 2, 
DAY(CTP_Date_of_accident__c) = 29, 
OR( 
MOD( YEAR(CTP_Date_of_accident__c), 400 ) = 0, 
AND( 
MOD( YEAR(CTP_Date_of_accident__c), 4 ) = 0, 
MOD( YEAR(CTP_Date_of_accident__c), 100 ) &lt;&gt; 0) 
) 
), 
DATE( YEAR(CTP_Date_of_accident__c) + 3 , 3, 1), 
DATE( YEAR(CTP_Date_of_accident__c) + 3, MONTH(CTP_Date_of_accident__c), DAY(CTP_Date_of_accident__c) ) 
))),true,false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Late Claim Flag</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
