        <errorConditionFormula>RecordType.DeveloperName=&apos;CTP_LogActivity&apos; &amp;&amp; ISBLANK(WhoId)</errorConditionFormula>
        <errorDisplayField>WhoId</errorDisplayField>
        <errorMessage>Please provide attending parties</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CTP_DueDateValidation</fullName>
        <active>true</active>
        <description>If Triage outcome = &quot;More triage&quot; the Next triage due date is mandatory</description>
        <errorConditionFormula>AND(ISPICKVAL(CTP_Triage_Outcome__c,&apos;More triage&apos;),ISNULL( CTP_Next_Triage_Due_date__c ))</errorConditionFormula>
        <errorDisplayField>CTP_Next_Triage_Due_date__c</errorDisplayField>
        <errorMessage>Next Triage Due Date is mandatory when Triage Outcome is &apos;More triage&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CTP_NextADRDueDate</fullName>
        <active>true</active>
        <description>IF ADR Outcome = &quot;More ADR&quot; thenÂ &apos;Next ADR Due date&apos; can&apos;t be blank.</description>
        <errorConditionFormula>AND(ISPICKVAL(CTP_ADR_Outcome__c,&apos;More ADR&apos;),ISNULL(CTP_Next_ADR_Due_date__c))</errorConditionFormula>
        <errorDisplayField>CTP_Next_ADR_Due_date__c</errorDisplayField>
        <errorMessage>&apos;Next ADR Due Date&apos; is mandatory when ADR Outcome is &apos;More ADR&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CTP_TaskDateValidation</fullName>
        <active>true</active>
        <description>DCR-287</description>
        <errorConditionFormula>CTP_Date__c &lt; DATE(2017,12,1)</errorConditionFormula>
        <errorDisplayField>CTP_Date__c</errorDisplayField>
        <errorMessage>Date cannot be prior to Dec 1 2017</errorMessage>
    </validationRules>
