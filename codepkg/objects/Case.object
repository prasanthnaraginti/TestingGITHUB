        <fullName>CTP_Age__c</fullName>
        <description>Age of Injured Person</description>
        <externalId>false</externalId>
        <formula>IF( NOT( ISBLANK( CTP_InjuredPersonContact__r.Birthdate ) ) ,
  IF( DATE( 2000 , MONTH( CTP_InjuredPersonContact__r.Birthdate ) , DAY( CTP_InjuredPersonContact__r.Birthdate) ) &lt;= DATE( 2000 , MONTH( TODAY() ) , DAY( TODAY() ) ),
     YEAR (Today()) - YEAR ( CTP_InjuredPersonContact__r.Birthdate ),
     YEAR (Today()) - YEAR ( CTP_InjuredPersonContact__r.Birthdate ) -1 ),
  null)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
