    <validationRules>
        <fullName>CTP_AccidentDateInPastForClaimsCase</fullName>
        <active>true</active>
        <description>When creating a Claims Case Date of Accident must be in the past</description>
        <errorConditionFormula>RecordType.DeveloperName  = &apos;CTP_Assist_Non_damages&apos; &amp;&amp;  CTP_Date_of_accident__c&gt;today()</errorConditionFormula>
        <errorMessage>Date of accident can not be a future date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CTP_Close_Reason_Required_On_Close</fullName>
        <active>true</active>
        <description>When a CTP Assist Agent closes a case, they must have provided a reason for closure</description>
        <errorConditionFormula>AND( OR(RecordType.DeveloperName = &apos;Feedback&apos;,RecordType.DeveloperName = &apos;CTP_Assist_Non_damages&apos;,RecordType.DeveloperName = &apos;CTP_Assist_Damages&apos;,RecordType.DeveloperName = &apos;CTP_Compensation_to_Relatives&apos;),ISPICKVAL(Status, &quot;Closed&quot;),(ISBLANK(TEXT(Case_Closed_Reason__c))))</errorConditionFormula>
        <errorDisplayField>Case_Closed_Reason__c</errorDisplayField>
        <errorMessage>Please select a reason before closing the case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CTP_Internal_Category_Required</fullName>
        <active>true</active>
        <description>For CTP Assist - Feedback cases (DCR-146), internal category field is required for certain Feedback sub-types and must be filled before being closed.</description>
        <errorConditionFormula>AND
( RecordType.DeveloperName = &quot;Feedback&quot;,ISPICKVAL(Status, &quot;Closed&quot;), OR(ISPICKVAL(CTP_Feedback_Sub_type__c ,&quot;CTP Green Slips&quot;),ISPICKVAL(CTP_Feedback_Sub_type__c ,&quot;Motor Accident Injury Claims&quot;),ISPICKVAL(CTP_Feedback_Sub_type__c ,&quot;Claims Disputes&quot;),ISPICKVAL(CTP_Feedback_Sub_type__c ,&quot;About My Insurer&quot;)),(ISBLANK(TEXT(CTP_Internal_Category__c)))
)</errorConditionFormula>
        <errorDisplayField>CTP_Internal_Category__c</errorDisplayField>
        <errorMessage>Please select the internal category before closing the case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CTP_LodgementDateGreaterThanAccidentDate</fullName>
        <active>true</active>
        <description>When creating a Claims Case Date of Lodgement must NOT be before the Date of Accident</description>
        <errorConditionFormula>AND( OR(RecordType.DeveloperName  = &apos;CTP_Assist_Non_damages&apos;,RecordType.DeveloperName  = &apos;CTP_Assist_Damages&apos;,RecordType.DeveloperName  = &apos;CTP_Compensation_to_Relatives&apos;) , CTP_Date_of_accident__c &gt; CTP_LodgementDate__c)</errorConditionFormula>
        <errorDisplayField>CTP_LodgementDate__c</errorDisplayField>
        <errorMessage>Date of Logdement can not be before the Date of Accident</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CTP_LodgementDatePastDate</fullName>
        <active>true</active>
        <description>When creating a Claims Case Date of Lodgement must be in the past</description>
        <errorConditionFormula>AND( OR(RecordType.DeveloperName  = &apos;CTP_Assist_Non_damages&apos;,RecordType.DeveloperName  = &apos;CTP_Assist_Damages&apos;,RecordType.DeveloperName  = &apos;CTP_Compensation_to_Relatives&apos;) ,CTP_LodgementDate__c&gt;today())</errorConditionFormula>
        <errorDisplayField>CTP_LodgementDate__c</errorDisplayField>
        <errorMessage>Please enter a valid date</errorMessage>
    </validationRules>
</CustomObject>
