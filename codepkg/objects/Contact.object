    <validationRules>
        <fullName>CTP_DOBFutureDate</fullName>
        <active>true</active>
        <description>Date of Birth Must be in the past</description>
        <errorConditionFormula>Birthdate&gt;=TODAY()</errorConditionFormula>
        <errorMessage>Date of Birth can not be a future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CTP_LanugageIfInterpreterRequired</fullName>
        <active>true</active>
        <description>Language is required when an Interpreter is Required</description>
        <errorConditionFormula>OR( 
AND(ISPICKVAL( InterpreterRequired__c , &quot;Yes&quot;) , 
OR (ISBLANK( CTP_Claimant_Language_Code__c ),ISNULL( CTP_Claimant_Language_Code__c ))  )
)</errorConditionFormula>
        <errorDisplayField>Language__c</errorDisplayField>
        <errorMessage>Language is required when an Interpreter is Required</errorMessage>
    </validationRules>
